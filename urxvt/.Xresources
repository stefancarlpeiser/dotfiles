!------------------------------------------------------------------------------
! c o l o r -  s e t t i n g s

*background: #222222
*foreground: #777777

! Default
! 0: black
*color0:  #222222
*color8:  #222222
! 1: red
*color1:  #9f6767
*color9:  #9f6767
! 2: green
*color2:  #92ac68
*color10: #92ac68
! 3: yellow
*color3:  #d0d293
*color11: #d0d293
! 4: blue
*color4:  #9aacc3
*color12: #9aacc3
! 5: magenta
*color5:  #bb77a4
*color13: #bb77a4
! 6: cyan
*color6:  #77bbb5
*color14: #77bbb5
! 7: white
*color7:  #777777
*color15: #777777

!------------------------------------------------------------------------------
! u r x v t
URxvt*scrollBar:       false
URxvt*scrollBar_right: true
URxvt*scrollColor:     #000000
URxvt.transparent: true
URxvt.shading: 20
URxvt.foreground: #eeeeee
URxvt.perl-ext-common: keyboard-select, matcher, resize-font,fullscreen, selection-to-clipboard
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.keysym.C-minus: perl:resize-font:smaller
URxvt.keysym.C-plus: perl:resize-font:bigger:
URxvt.keysym.C-equal: perl:resize-font:reset
URxvt.url-launcher: /usr/bin/google-chrome
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
